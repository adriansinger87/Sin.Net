<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <RepositoryUrl>https://github.com/adriansinger87/Sin.Net</RepositoryUrl>
    <PackageProjectUrl>https://github.com/adriansinger87/Sin.Net</PackageProjectUrl>
    <Copyright>Adrian Singer</Copyright>
    <PackageLicenseUrl>https://opensource.org/licenses/MIT</PackageLicenseUrl>
    <Authors>Adrian Singer</Authors>
    <Version>0.0.1</Version>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Description>Sin.Net.Persistence contains a reference implementation of the Sin.Net.Domain interfaces for im- and export. Supported  formats are Json and Binary. A derivation of the features in your client code is supported. More features will follow.</Description>
    <PackageTags>persistence json binary</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="12.0.1" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Sin.Net.Domain\Sin.Net.Domain.csproj" />
    <ProjectReference Include="..\Sin.Net.Logging\Sin.Net.Logging.csproj" />
  </ItemGroup>

</Project>
